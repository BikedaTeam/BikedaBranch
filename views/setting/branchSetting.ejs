<!DOCTYPE html>
<html>
<head>
  <%- include ('../header')%>
</head>
<body class="hold-transition skin-blue sidebar-mini">
  <div class="wrapper">

    <%- include ('../topMenu')%>
    <!-- Left side column. contains the logo and sidebar -->
    <%- include ('../sideMenu')%>
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <section class="content-header">
        <h1>
          지점설정
          <small>지점관련 설정</small>
        </h1>
      </section>

      <!-- Main content -->
      <section class="content">
        <!-- 계좌관리 -->
        <div class="col-lg-6">
          <div class="box box-info">
            <div class="box-header with-border">
              <h3 class="box-title">계좌관리</h3>
            </div>
            <div class="form-horizontal">
              <div class="box-body">
                <div class="col-lg-12">

                  <div class="form-group">

                    <div class="col-sm-6">
                      <div class="input-group">
                        <span class="input-group-addon" style="min-width: 90px;">
                          <select class="form-control select2">
                            <option selected="selected">경남</option>
                            <option>우리</option>
                            <option>국민</option>
                          </select>
                        </span>
                        <input type="input" class="form-control" placeholder="'-'없이 입력">
                      </div>
                    </div>

                    <label for="inputEmail3" class="col-sm-1 control-label">포인트</label>

                    <div class="col-sm-3">
                      <input type="input" class="form-control" id="inputEmail3" readonly>
                    </div>

                  </div>

                  <div class="form-group">

                    <div class="col-sm-push-11 col-sm-1">
                      <button type="button" class="btn btn-sm btn-info">저장</button>
                    </div>

                  </div>

                </div>
              </div>
            </div>
          </div>
        </div>

        <!-- 공유지점 관리 -->
        <div class="col-lg-6">
          <div class="box box-info">
            <div class="box-header with-border">
              <h3 class="box-title">공유지점 관리</h3>
            </div>
            <div class="form-horizontal">
              <div class="box-body">

                <div class="col-lg-12">

                  <div class="form-group">

                    <label for="inputEmail3" class="col-sm-2 control-label">지점</label>

                    <div class="col-sm-6">
                      <select class="form-control select2">
                      </select>
                    </div>

                    <label for="inputEmail3" class="col-sm-2 control-label">지연시간</label>

                    <div class="col-sm-1">
                      <input type="input" class="form-control" id="inputEmail3">
                    </div>

                    <div class="col-sm-1">
                      <button type="button" class="btn btn-sm btn-info">추가</button>
                    </div>

                  </div>

                  <div class="form-group">

                    <!-- <div class="col-sm-push-11 col-sm-1">
                      <button type="button" class="btn btn-sm btn-info">삭제</button>
                    </div> -->

                    <table id="dataTable" class="table table-bordered table-striped">
                      <thead>
                        <tr>
                          <th>지점</th>
                          <th>지연시간</th>
                        </tr>
                      </thead>
                    </table>

                  </div>

                </div>

              </div>
            </div>
          </div>
        </section>
        <!-- /.content -->
      </div>
      <!-- /.content-wrapper -->
    </div>
    <!-- ./wrapper -->
    <%- include ('../footer')%>
    <script src="routes/setting/branchSetting.js"></script>
  </body>
  </html>
